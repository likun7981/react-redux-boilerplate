{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "bolierplate for react-redux",
  "main": "src/main.js",
  "scripts": {
    "start": "better-npm-run start",
    "compile": "better-npm-run compile",
    "lint": "eslint src",
    "flow": "flow",
    "lint:fix": "npm run lint -- --fix",
    "start:dist": "better-npm-run start:dist"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "starkit",
    "template"
  ],
  "betterScripts": {
    "start": {
      "command": "node bin/www",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "rimraf dist/* && npm run lint:fix && node bin/compile",
      "env": {
        "DEBUG": "app:*:compile*",
        "NODE_ENV": "production"
      }
    },
    "start:dist": {
      "command": "node bin/www",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    }
  },
  "pre-commit": [
    "lint"
  ],
  "author": "likun",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "^0.0.13",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "debug": "^2.4.5",
    "detect-port": "^1.1.1",
    "eslint": "^3.12.2",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "2.1.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.45.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.24.1",
    "internal-ip": "^1.2.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.2.1",
    "pre-commit": "^1.2.2",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.3"
  }
}